{
  "name": "@distributed/amon",
  "description": "Small library exposing utility methods for GraphQL",
  "version": "0.0.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/distributedvc/amon",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "user",
    "tools",
    "utility",
    "graphql",
    "models",
    "library"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "resolutions": {
    "eslint-config-prettier": "8.3.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=node",
    "lint": "eslint --ext .ts src/",
    "lint:tsdx": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "module": "dist/amon.esm.js",
  "size-limit": [
    {
      "path": "dist/amon.cjs.production.min.js",
      "limit": "200 KB"
    },
    {
      "path": "dist/amon.esm.js",
      "limit": "200 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.7.2",
    "@vercel/node": "^1.12.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "fastify": "^3.20.2",
    "husky": "^7.0.2",
    "prettier": "^2.3.2",
    "size-limit": "^5.0.3",
    "standard-version": "^9.3.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1"
  }
}
